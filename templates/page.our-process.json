    "e6b65440-64f9-471d-a97a-e6ae7c5dfde2": {
      "type": "rich-text",
      "settings": {
        "id": "richtext-heading",
        "classes": "remove-vertical-spacing top bottom",
        "subheading": "",
        "title": "",
        "content": "<p>Our carefully curated Signature Range is bespoke, made to order within 4-6 weeks. We work with you to co-create your dream.<br\/>Your vision, our expertise. Let's walk this special journey together.<\/p>",
        "button_text": "",
        "button_link": "",
        "background_type": "full_width",
        "text_width": "large",
        "text_position": "center",
        "text_alignment": "center",
        "background": "#f8f7f2",
